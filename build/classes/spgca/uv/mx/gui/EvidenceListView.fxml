<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="602.0" prefWidth="827.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sgpca.uv.mx.gui.EvidenceListController">
   <children>
      <Label layoutX="32.0" layoutY="34.0" text="Evidence list">
         <font>
            <Font size="20.0" />
         </font></Label>
      <TableView fx:id="tableEvidenceList" layoutX="32.0" layoutY="117.0" prefHeight="365.0" prefWidth="773.0">
        <columns>
          <TableColumn fx:id="columnName" prefWidth="218.0" text="Name" />
          <TableColumn fx:id="columnAutor" prefWidth="187.0" text="Autor" />
            <TableColumn fx:id="columnType" prefWidth="111.0" text="Type" />
            <TableColumn fx:id="columnDate" prefWidth="91.0" text="Date" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="buttonSearchEvidence" layoutX="717.0" layoutY="72.0" mnemonicParsing="false" onAction="#searchEvidence" prefHeight="30.0" prefWidth="82.0" text="Search" />
      <TextField fx:id="textFieldSearchEvidence" layoutX="383.0" layoutY="72.0" prefHeight="30.0" prefWidth="316.0" promptText="Evidence Name" />
      <Button fx:id="buttonRegisterEvidence" layoutX="651.0" layoutY="509.0" mnemonicParsing="false" onAction="#registerEvidence" prefHeight="49.0" prefWidth="154.0" text="Register evidence">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="buttonExitEvidenceList" layoutX="32.0" layoutY="509.0" mnemonicParsing="false" onAction="#exitEvidenceList" prefHeight="49.0" prefWidth="85.0" text="Exit">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
